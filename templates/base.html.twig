            <link rel="stylesheet" href="/css/styles.css">
        {% endblock %}
    </head>
    <body>

        {% include 'partials/navbar.html.twig' %}
        <div class="container pt-5">
            <div class="row">
                <div class="col-md-8"> {% block body %}{% endblock %}
